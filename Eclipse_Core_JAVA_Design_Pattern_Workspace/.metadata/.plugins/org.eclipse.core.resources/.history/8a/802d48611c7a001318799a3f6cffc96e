package com.pranjay;

import com.pranjay.Person.PersonBuilder;

public class BuildPersonObject {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
PersonBuilder personBuilder=new Person.PersonBuilder("pranjay","singh");
personBuilder.setEmploymentType("employes");
personBuilder.setMarried("yes");
personBuilder.buildPerson(personBuilder);
	}

}
